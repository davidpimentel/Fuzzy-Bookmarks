{
  "name": "Fuzzy Bookmarks",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Fuzzy search all of your bookmarks",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "commands": {
    "open-popup": {
      "suggested_key": {
        "windows": "Ctrl+B",
        "mac": "Command+B",
        "chromeos": "Ctrl+B",
        "linux": "Ctrl+B"
      },
      "description": "open bookmark search popup"
    }
    },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ]
}
